    "connect-multiparty": "^2.2.0",
    "cookie-parser": "~1.3.5",
    "cron": "^1.8.2",
    "cross-env": "^5.2.1",
    "crypto-js": "^4.1.1",
    "debug": "^4.3.1",
    "dotenv": "^10.0.0",
    "exceljs-node": "^1.6.7",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "formidable": "^1.2.2",
    "fresh": "^0.5.2",
    "glob": "^5.0.14",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "mongoose": "^5.12.13",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "negotiator": "^0.6.1",
    "node-libcurl": "^2.3.3",
    "node-serialize": "0.0.4",
    "nodemailer": "^6.6.3",
    "number_format-php": "^1.0.14",
    "pdfkit": "^0.12.3",
    "php-serialization": "0.0.4",
    "pug": "^3.0.2",
    "query-string": "^7.0.1",
    "serve-favicon": "^2.5.0",
    "socket.io": "^4.2.0",
    "sprintf-js": "^1.1.2",
    "tslint-config-prettier": "^1.18.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5",
    "xmlbuilder": "^15.1.1",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/chai": "^4.2.18",
    "@types/cookie-parser": "^1.4.2",
    "@types/cron": "^1.7.3",
    "@types/crypto-js": "^4.0.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.12",
    "@types/express-formidable": "^1.0.5",
    "@types/formidable": "^1.2.3",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/lodash": "^4.14.170",
    "@types/mocha": "^5.2.0",
    "@types/mysql": "^2.15.19",
    "@types/mysql2": "github:types/mysql2",
    "@types/node": "^8.10.66",
    "@types/nodemailer": "^6.4.4",
    "@types/nodemailer-direct-transport": "^1.0.32",
    "@types/nodemailer-smtp-transport": "^2.7.5",
